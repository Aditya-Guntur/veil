{
  "canisters": {
    "mempool_chess_backend": {
      "type": "rust",
      "candid": "mempool_chess_backend.did",
      "package": "mempool_chess_backend",
      "build": "cargo build --target wasm32-unknown-unknown --release --no-default-features --features vetkeys"
    },
    "vetkeys_engine": {
    "type": "custom",
    "wasm": "vetkeys_engine/target/wasm32-unknown-unknown/release/vetkeys_engine.wasm",
    "candid": "vetkeys_engine/vetkeys_engine.did"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
